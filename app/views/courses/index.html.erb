<h3>Courses</h3>
<div class="span8">
<%= will_paginate %>
<table class="table table-condensed">
    <thead>
      <tr>
          <th>Number</th>
          <th>Title</th>
          <th>Instructor</th>
          <th>Status</th>
          <th>Available Seats</th>
          <th>Last Updated</th>
          <th>Notification</th>
          <th></th>
          <th></th>
      </tr>
  </thead>
  <tbody>
  <% @courses.each do |course| %>
    <%= render course %>
  <% end %>
  </tbody>
</table>
<%= will_paginate %>
</div>